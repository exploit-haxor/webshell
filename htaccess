#Rewrite URL
<IfModule mod_rewrite.c>
    <IfModule mod_negotiation.c>
        Options -MultiViews
    </IfModule>
	
	RewriteEngine On
	RewriteRule ^([a-z0-9_-]+)/$ index.php?id=$1 [QSA,L]
</IfModule>

#Rewrite URL
<IfModule mod_rewrite.c>
RewriteEngine on

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule [^/]$ %{REQUEST_URI}/ [L,R=301]
</IfModule>
